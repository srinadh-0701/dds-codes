// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int fact(int num){
    if (num==1){
        return 1;
    }
    else{
        return num*fact(num-1);
    }
}
int main(){
    int n;
    cout<<"enter any number:";
    cin>>n;
    int ans=fact(n);
    cout<<ans;
}
