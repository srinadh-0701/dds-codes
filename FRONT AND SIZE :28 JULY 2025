// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
#define size 4
int Queue[size];
int front=-1,rear=-1;
void enqueue(int num){
    if(front==0&&rear== size-1||(rear+1)%size==front){
        cout<<"Queue is full can not insert \n";
    }
    else if(front==-1&&rear==-1){
        front=rear=0;
        Queue[rear]= num;
        cout<<num<<"inserted"<<endl;
    }
    else{
        rear=(rear+1)%size;
        Queue[rear]=num;
        cout<<num<<"iserted"<<endl;
    }
}
int main()
{
    enqueue(10);
    enqueue(20);
    enqueue(30);
    enqueue(40);
    enqueue(50);
}
